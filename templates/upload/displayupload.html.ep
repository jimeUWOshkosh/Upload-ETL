  <TR><TH>File</TH><TH>Size</TH></TR>
     % for my $fi (@$files) {
        <TR>
           <TD><%= $fi->{name} %></TD><TD><%= $fi->{size} %></TD>
        </TR>
     % }
</TABLE>
